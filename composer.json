{
    "name": "bristol-su/unioncloud-portal",
    "description": "An integration with unioncloud for use with the portal",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Toby Twigger",
            "email": "tobytwigger1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "bristol-su/support": "dev-portal-v4 as 4.7.2",
        "twigger/unioncloud": "dev-develop as 1.0.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BristolSU\\UnionCloud\\UnionCloudIntegrationServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "BristolSU\\UnionCloud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BristolSU\\Tests\\UnionCloud\\": "tests/"
        }
    }
}
